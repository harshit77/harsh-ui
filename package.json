{
  "name": "harsh-ui",
  "version": "1.0.0",
  "private": false,
  "main": "./index.js",
  "module": "./index.js",
  "files": [
    "index.js",
    "icons.js",
    "hooks.js"
  ],
  "dependencies": {
    "@storybook/test": "^8.0.6",
    "react-datepicker": "^4.11.0",
    "react-dropzone": "^14.2.3",
    "react-file-viewer": "^1.2.1",
    "react-simple-maps": "^3.0.0",
    "recharts": "^2.5.0"
  },
  "scripts": {
    "prebuild-lib": "rimraf src/lib/icons && rimraf dist",
    "storybook": "storybook dev -p 6006 ./src",
    "test-storybook": "test-storybook",
    "build-storybook": "storybook build",
    "build": "npm pack --pack-destination='./build'",
    "build-lib": "npm run svgr && rollup -c --bundleConfigAsCjs",
    "svgr": "svgr --config-file .svgrrc.js --out-dir src/lib/icons assets"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.1",
    "@rollup/plugin-url": "^8.0.1",
    "@storybook/addon-actions": "^8.0.6",
    "@storybook/addon-essentials": "^8.0.6",
    "@storybook/addon-interactions": "^8.0.6",
    "@storybook/addon-links": "^8.0.6",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addons": "^7.6.17",
    "@storybook/manager-api": "^8.0.6",
    "@storybook/node-logger": "^8.0.6",
    "@storybook/preset-create-react-app": "^8.0.6",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^8.0.6",
    "@storybook/react-webpack5": "^8.0.6",
    "@storybook/theming": "^8.0.6",
    "@svgr/cli": "^6.5.1",
    "@svgr/rollup": "^6.5.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@tippyjs/react": "^4.2.6",
    "babel-plugin-named-exports-order": "0.0.2",
    "classnames": "^2.3.2",
    "css-loader": "5.2.7",
    "postcss": "^8.4.21",
    "postcss-url": "^10.1.3",
    "prop-types": "^15.8.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "5.0.1",
    "react-toastify": "^9.1.1",
    "resolve-url-loader": "^5.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^3.2.5",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-size": "^0.2.2",
    "sass": "^1.26.11",
    "sass-loader": "10.1.1",
    "storybook": "^8.0.6",
    "storybook-css-modules-preset": "^1.1.1",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.74.0"
  },
  "peerDependencies": {
    "dompurify": "^3.0.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-router-dom": "^5.0.1",
    "react-toastify": "^9.1.1"
  }
}
